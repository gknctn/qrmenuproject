<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>DASHMIN - Bootstrap Admin Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="~/dashmin_Admin_Panel/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/dashmin_Admin_Panel/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/dashmin_Admin_Panel/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/dashmin_Admin_Panel/css/style.css" rel="stylesheet">
</head>

<body>
    <div class="container-fluid position-relative bg-white d-flex p-0">

        <!-- Sidebar Start -->
        <partial name="_AdminSidebar" />
        <!-- Sidebar End -->
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            <partial name="_AdminNavbar" />
            @RenderSection("Header", required: false)
            <!-- Navbar End -->
            <!-- Blank Start -->
            <div class="container-fluid p-4">
                <div class="col-md-12">
                    <h1 class="text-center">Admin Panel - @ViewData["Title"]</h1>
                </div>
                <div class="row vh-100 bg-light rounded justify-content-center mx-0">
                    <div class="col-sm-12 col-xl-12 p-4 rounded-5 border-top shadow" style="background-color: #f0f8ff">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!-- Blank End -->
            <!-- Footer Start -->
            <partial name="_AdminFooter" />
            <!-- Footer End -->
        </div>
        <!-- Content End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/chart/chart.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/chart/chart.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/easing/easing.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/waypoints/waypoints.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/dashmin_Admin_Panel/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="https://kit.fontawesome.com/5e77878a5e.js" crossorigin="anonymous"></script>
    <!-- Template Javascript -->
    <script src="~/dashmin_Admin_Panel/js/main.js"></script>
</body>

</html>